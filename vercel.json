{
  "functions": {
    "api/create-checkout.js": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/webhook.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/create-checkout",
      "dest": "/api/create-checkout.js",
      "methods": ["POST", "OPTIONS"]
    }
  ]
}
